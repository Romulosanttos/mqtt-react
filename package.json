{
  "name": "react-mqtt",
  "version": "0.0.11",
  "description": "Connecting MQQT to React Components",
  "main": "./lib/index.js",
  "scripts": {
    "build:lib": "set NODE_ENV=development&& babel src --out-dir lib",
    "build:umd": "set NODE_ENV=development&& webpack --config=webpack.config.js src/index.js dist/mqtt-react.js",
    "build:umd:min": "set NODE_ENV=production&& webpack --config=webpack.config.js src/index.js dist/mqtt-react.min.js",
    "build": "npm run build:lib && npm run build:umd",
    "test": "cross-env NODE_ENV=test ava"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Romulosanttos/mqtt-react"
  },
  "keywords": [
    "react",
    "mqtt"
  ],
  "author": {
    "name": "RÃ´mulo Cabral Santos",
    "url": "http://romulosanttos.github.io",
    "email": "romulosanttos1@gmail.com"
  },
  "license": "MIT",
  "ava": {
    "files": [
      "./test/*.spec.js"
    ],
    "failFast": true,
    "tap": true,
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {
    "mqtt": "^2.7.0",
    "object.omit": "^3.0.0"
  },
  "peerDependencies": {
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "prop-types": "^15.5.8"
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "ava-spec": "^1.1.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "object.omit": "^3.0.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.0",
    "react-dom": "^15.5.0",
    "react-test-renderer": "^15.5.4",
    "sinon": "^2.2.0",
    "webpack": "^2.4.1"
  }
}
